<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wisp.game.bet.api.dao.log.BindYbLogDao">

    <sql id="bndybLogColumns">
        a.id AS "id",
        a.cid AS "cid",
        a.uid AS "uid",
        a.tm AS "tm",
        a.sub AS "sub",
        a.parcnt AS "parcnt",
        a.tm AS "tm"
    </sql>

    <sql id="bndYbLogJoins">
    </sql>

    <select id="findById" resultType="com.wisp.game.bet.api.dao.log.entity.BindYbLogEntity">
        SELECT
        <include refid="bndybLogColumns"/>
        FROM bndyb_log a
        <include refid="bndYbLogJoins"/>
        WHERE a.id = #{id}
    </select>


</mapper>